{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmit": true,
    "declaration": false,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true
  }
}
